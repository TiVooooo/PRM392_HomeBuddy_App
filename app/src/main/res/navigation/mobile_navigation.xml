<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.prm392_homebuddy_app.ui.account.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.prm392_homebuddy_app.ui.cart.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/navigation_cleanup"
        android:name="com.example.prm392_homebuddy_app.ui.service.CleanUpFragment"
        android:label="fragment_clean_up"
        tools:layout="@layout/fragment_clean_up" />
    <fragment
        android:id="@+id/navigation_order"
        android:name="com.example.prm392_homebuddy_app.ui.order.OrderFragment"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.prm392_homebuddy_app.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>